<div class="newListing">
    <form method="POST" action="/profile/listings/<%=creatId%>/?_method=PUT">
        <label>Name</label>
        <input type="text" name="name" value="<%= listing.name %>">
        <label>Location</label>
        <input type="text" name="location" value="<%= listing.location %>">
        <label>Tags</label>
        <input type="text" name="tags" value="<%= listing.tags %>">
        <input type="submit">
    </form>
</div>
