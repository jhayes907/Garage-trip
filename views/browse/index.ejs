<h1>This is the browse page</h1>


<div class="row">
  <table class="table table-bordered table-striped">
      <thead>
          <th>Name</th>
          <th>Id</th>
          <th>Location</th>
          <th>Tags</th>
      </thead>
      <tbody>
          <% if(Array.isArray(listings) && listings.length > 0) { %>
              <% listings.forEach((listing) => { %>
                  <tr> 
                      <td><a href="/browse/show/<%= listing.id %>">
                          <%= listing.name %>
                      </a></td>
                      <td><%= listing.id %></td>
                      <td><%= listing.location %></td>
                      <td><%= listing.tags %></td>
                      <td><%= listing.content %></td>
                  </tr>
              <% }) %>
          <% } %>
      </tbody>
  </table>
</div> 